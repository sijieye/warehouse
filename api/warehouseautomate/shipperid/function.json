{
  "bindings": [
    {
      "name": "req",
      "authLevel": "function",
      "methods": [
        "get"
      ],
      "direction": "in",
      "type": "httpTrigger",
      "route": "shipments/shipperid/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "cosmosDB",
      "direction": "in",
      "type": "cosmosDB",
      "connectionStringSetting": "warehouseKey",
      "databaseName": "warehouse",
      "collectionName": "shipment",
      "sqlQuery": "SELECT * from shipment s where s.ShipperID = {id}"
    }
  ]
}